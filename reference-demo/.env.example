# Permission Protocol Configuration
# Copy this to .env and fill in your values

# Permission Protocol API endpoint
PP_ENDPOINT=https://your-pp-instance.com

# API key for authentication (must have permission:execute scope)
PP_API_KEY=your-api-key-here

# Tenant/Company ID
PP_TENANT_ID=your-tenant-id

# Agent ID for the demo
PP_AGENT_ID=demo-agent
